{
    "manifest_version": 3,
    "name": "Popup Extension",
    "version": "1.0",
    "description": "A simple Chrome extension with a popup",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/LargeBackground.png",
        "48": "images/LargeBackground.png",
        "128": "images/LargeBackground.png"
      }
    },
    "content_scripts": [
        {
          "js": ["googleTest.js","listeners.js"],
          "matches": ["<all_urls>"]
        }
    ],
    "permissions": [
        "activeTab",
        "storage",
        "scripting",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    }
  }